<div class="jumbotron">
    <img src ="assets/css/news-scan-bg.jpg">
</div>
<div id = "scraped" class="panel panel-default">
  {{#each article }}
  <div class="panel-heading">
    <h3 class="panel-title">{{this.date}}, {{this.title}}</h3>
  </div>
  <div class="panel-body">
    {{this.description}}
  </div>
  <div class="panel-footer" placeholder="link to article">{{this.link}}</div>
  </div>
  <button class="btn btn-primary float-right btn-saved" data-id = {{this._title}}>Save Article</button>
  {{/each}}
  
